import java.util.*;
class abc
{
int x,y,z;
public abc(int a,int b, int c){
x=a;
y=b;
z=c;
System.out.println(x+y+z);
}
}
class sum1
{
public static void main(String args[]){
abc n1=new abc(4,5,6);
abc n2=new abc(1,2,3);
abc n3=new abc(44,55,66);
abc n4=new abc(43,22,1);
abc n5=new abc(14,54,61);
}
}
